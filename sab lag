task.wait(1)

local players = game:GetService("Players")
local lighting = game:GetService("Lighting")
local coreGui = game:GetService("CoreGui")

local localPlayer = players.LocalPlayer
local playerBackpack = localPlayer:WaitForChild("Backpack")

pcall(function()
	local renderingSettings = getfenv().settings()
	renderingSettings.Rendering.QualityLevel = Enum.QualityLevel.Level01
end)

pcall(function()
	local gameGraphicsSettings = UserSettings().GameSettings
	gameGraphicsSettings.SavedQualityLevel = Enum.SavedQualitySetting.QualityLevel1
	gameGraphicsSettings.GraphicsQualityLevel = 1
end)

lighting.GlobalShadows = false
lighting.EnvironmentDiffuseScale = 0
lighting.EnvironmentSpecularScale = 0

for _, lightingEffect in ipairs(lighting:GetChildren()) do
	if lightingEffect:IsA("PostEffect") then
		lightingEffect.Enabled = false
	end
end

for _, visualObject in ipairs(workspace:GetDescendants()) do
	if visualObject:IsA("ParticleEmitter")
	or visualObject:IsA("Trail")
	or visualObject:IsA("Beam") then
		pcall(function()
			visualObject.Enabled = false
		end)
	end
end

local function removeCharacterAccessories(characterModel)
	for _, characterChild in ipairs(characterModel:GetChildren()) do
		if characterChild:IsA("Accessory") then
			characterChild:Destroy()
		end
	end
	characterModel.ChildAdded:Connect(function(newChild)
		if newChild:IsA("Accessory") then
			newChild:Destroy()
		end
	end)
end

for _, humanoidInstance in ipairs(workspace:GetDescendants()) do
	if humanoidInstance:IsA("Humanoid") and humanoidInstance.Parent then
		removeCharacterAccessories(humanoidInstance.Parent)
	end
end

workspace.DescendantAdded:Connect(function(newDescendant)
	if newDescendant:IsA("Humanoid") and newDescendant.Parent then
		removeCharacterAccessories(newDescendant.Parent)
	end
end)

local screenGui = Instance.new("ScreenGui")
screenGui.Name = "TigyFPSDevourUI"
screenGui.ResetOnSpawn = false
screenGui.Parent = coreGui

local mainFrame = Instance.new("Frame")
mainFrame.Parent = screenGui
mainFrame.Size = UDim2.new(0, 340, 0, 160)
mainFrame.Position = UDim2.new(0.5, -170, 0.5, -80)
mainFrame.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
mainFrame.BorderSizePixel = 0
mainFrame.Active = true
mainFrame.Draggable = true

Instance.new("UICorner", mainFrame).CornerRadius = UDim.new(0, 18)

local titleLabel = Instance.new("TextLabel")
titleLabel.Parent = mainFrame
titleLabel.Size = UDim2.new(1, -20, 0, 40)
titleLabel.Position = UDim2.new(0, 10, 0, 10)
titleLabel.BackgroundTransparency = 1
titleLabel.Text = "KIRA KILLER FPSr"
titleLabel.Font = Enum.Font.GothamBlack
titleLabel.TextSize = 20
titleLabel.TextXAlignment = Enum.TextXAlignment.Left
titleLabel.TextColor3 = Color3.new(1, 1, 1)

local executeButton = Instance.new("TextButton")
executeButton.Parent = mainFrame
executeButton.Size = UDim2.new(1, -40, 0, 54)
executeButton.Position = UDim2.new(0, 20, 0, 80)
executeButton.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
executeButton.Text = "FPS Devour (need auras)"
executeButton.Font = Enum.Font.GothamBold
executeButton.TextSize = 15
executeButton.TextColor3 = Color3.new(1, 1, 1)
executeButton.BorderSizePixel = 0
executeButton.AutoButtonColor = true

Instance.new("UICorner", executeButton).CornerRadius = UDim.new(0, 14)

executeButton.MouseButton1Click:Connect(function()
	executeButton.Text = "WORKING..."
	executeButton.AutoButtonColor = false

	local character = localPlayer.Character
	if not character then return end

	local humanoid = character:FindFirstChildOfClass("Humanoid")
	if not humanoid then return end

	local quantumClonerTool = playerBackpack:FindFirstChild("Quantum Cloner")
	if quantumClonerTool then
		humanoid:EquipTool(quantumClonerTool)
		task.wait()
		for _, backpackTool in ipairs(playerBackpack:GetChildren()) do
			if backpackTool:IsA("Tool") then
				backpackTool.Parent = character
			end
		end
		task.wait()
		quantumClonerTool:Activate()
	end

	task.delay(0.5, function()
		executeButton.Text = "FPS Devour KIRA (need auras)"
		executeButton.AutoButtonColor = true
	end)
end)
